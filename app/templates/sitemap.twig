{% extends "layout/layout.twig" %}

{% block title_text %}{{ __('Sitemap') }}{% endblock %}

{% block page_content %}
    <h1>{{ __('Sitemap') }}</h1>
    <ul>
        {% for entry in entries %}
            {% if entry.link is defined %}
                <li><a href="{{ entry.link }}">{{ entry.title }}</a></li>
            {% endif %}
        {% endfor %}
    </ul>
{% endblock %}